<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Facebook">
    <listeners>
        <listener class-name="commons.MethodListener" />
    </listeners>
    <parameter name="url" value="https://www.facebook.com/r.php"></parameter>
    <test name="Run on CHROME">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.facebook.register.Level_12_Register_Login_Assert_Verify" />
        </classes>
    </test>
</suite>